      @IF validate.result {
        @CALL verifone_api.schedule_support {
          api_request: "curl -X POST {config.verifone.tech_support_endpoint} -H 'Authorization: Bearer {config.verifone.api_auth}' -d '{dcrs_id, issue_type, preferred_time}'"
